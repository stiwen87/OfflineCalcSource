@page "/shield"

<h3>Shield</h3>

@code {
    private static string[][] rawCosts = [["10", "10000", "1 aa", "10 ee"],
        ["10", "10 b", "10 bb", "100 dd"]];
    private static BigNum[][] Costs = rawCosts.Select(x => x.Select(y => new BigNum(y)).ToArray()).ToArray();

    private static double[][] basicOutputs = [[4,2,.889,.37],
        [12,6,2.67,1.11]];
    private static double[][] basicCommonPowers = [[1, 1, 1, 1],
        [1, 1, 1, 1]];
    private int[] generatorCount = basicCommonPowers.Select(x => x.Length).ToArray();
    private static double[] rarePowers = [2.5, 0];
    private int[] commonMulti = [4,3];
    private int rareMulti = 2;
    private bool[] canCrit = [false, true];
    private int[] critChance = [0, 13, 28, 45, 64, 85];
    private int basicCritBonus = 16;
}


<Calcs IndustryCount="2" GeneratorCount="generatorCount" Costs="Costs" basicOutputs="basicOutputs" basicCommonPowers="basicCommonPowers" rarePowers="rarePowers" commonMulti="commonMulti" rareMulti="rareMulti" critChance="critChance" basicCritBonus="basicCritBonus" canCrit="canCrit"/>